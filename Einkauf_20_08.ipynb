    "einkaufsliste = []\n",
    "\n",
    "while True:\n",
    "    produkt = input(\"Bitte Produkt eingeben (oder 'Fertig' zum Beenden): \")\n",
    "    if produkt == \"Fertig\":\n",
    "        break\n",
    "    einkaufsliste.append(produkt)\n",
    "\n",
    "print(\"Dein Einkauf für heute: \", einkaufsliste)"
